{
  "manifest_version": 3,

  "name": "End Game Spoiler",
  "description": "Trying to save you from End Game Spoiler.",
  "version": "1.3",
  "icons": { 
          "96": "End-game.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.4.0.min.js", "hide-spoiler.js"],
      "run_at": "document_end"
    }
  ]
}
